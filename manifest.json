{
    "name": "语音闹钟",
    "description": "定时语音弹窗提醒",
    "key": "",
    "manifest_version": 2,
    "default_locale": "zh_CN",
    "options_page": "options.html",
    "permissions": ["notifications", "tts", "background", "https://*.google.com.hk/*", "tabs","downloads"],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.4.4",
    "background": {
        "scripts": ["js/jquery.js", "js/background.js"]
    },
    "browser_action": {
        "default_icon": "images/48.png"
    },
    "icons": {
        "128": "images/128.png",
        "16": "images/16.png",
        "48": "images/48.png"
    },
    "web_accessible_resources": ["images/tx.png"]
}
